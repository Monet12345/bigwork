<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bigwork.bigwork_meta.dal.mapper.IdManagementMapper">
    <select id="getByKey" resultType="Long">
        select value
        from id_management_db
        where head = #{head}
    </select>
    <update id="update">
        update id_management_db
        set value = #{value} ,  gmt_update = #{updateTime}
        where head = #{head}
    </update>
    <insert id="insert">
        insert into id_management_db(head,value,gmt_create,gmt_update)
        values(#{head},#{value},#{createTime},#{updateTime})
    </insert>



</mapper>