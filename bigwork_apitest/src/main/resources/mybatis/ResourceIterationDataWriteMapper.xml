<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bigwork.bigwork_apitest.dal.mapper.ResourceIterationDataWriteMapper">



    <sql id="columns">
        resouce_data_id,
        now_iteration,
        last_iteration
    </sql>


    <resultMap id="resultMap" type="ResourceIterationDo">
        <id property="resourceDateId" column="resouce_data_id"/>
        <result property="nowIteration" column="now_iteration"/>
        <result property="lastIteration" column="last_iteration"/>

    </resultMap>
    <insert id="createIteration">
        insert into resource_iteration_data_write_db(
        <include refid="columns"/>
        )
        values(
        #{resourceDateId},
        #{nowIteration},
        #{lastIteration}
        )
    </insert>





</mapper>